create:/pickblock = >>>
    @player = player()
    try{
        @blockLoc = pcursor(@player)
    }catch(Exception @e){die()}

    @block = get_block_at(@blockLoc['x'],@blockLoc['y'],@blockLoc['z'],@blockLoc['world'])

    pgive_item(@player,@block,1)

<<<

