apocalypse:/apocalypse = >>>
    sudo(/effect player() 5 9999 20 true)
    sudo(/effect player() 10 9999 5 true)
    sudo(/effect player() 11 9999 10 true)
    sudo(/effect player() 12 9999 1 true)
    sudo(/effect player() 13 9999 1 true)
    sudo(/effect player() 21 9999 19 true)
    runas(~console,/give player() "minecraft:iron_boots 1 8:30 name:&4S&ep&4e&ce&ed&8s&0t&9e&1r_&4Boots")
    tmsg(player(),concat(color(red),
    'I cannot be defeated, don't try.'))
<<<

# TPs Apocalypse to the nether (aka Underworld)
underworld:/underworld = >>>
    sudo(/mv tp 1_9_nether)
<<<

# grow into giant
grow:/grow = >>>
    sudo(/disguise giant)
<<<
shrink:/shrink = >>>
   include('../includes/changeskin.ms')
   _changeSkin(player(),'apocalypse') 
<<<

# Gives player strength 10 for 5 minutes
empower:/empower $plr = >>>
    @plr = $plr
    sudo(/effect @plr 5 300 10 true)
    tmsg(player(), "You have strengthened ".@plr."!")
    tmsg(@plr, "You have been strengthened by ". player() ."!")
<<<
